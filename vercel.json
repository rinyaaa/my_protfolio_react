{
    "rewrites": [],
    "redirects": [],
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [
      {
        "src": "/path/to/javascript/files/*.js",
        "headers": {
          "Content-Type": "text/javascript"
        }
      }
    ],
    "headers": [
      {
        "source": "/my_protfolio_react/assets/(.*).js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }
  